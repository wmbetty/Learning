/* pages/details/details.wxss */
page{
    background: #F5F6F8;position: relative;height: 100%;overflow-y: hidden;
}
.page-head{
    padding: 20rpx 30rpx 40rpx;color: #343434;font-size: 60rpx;background: #F5F6F8;
}
.main-head{
    display: flex;justify-content: space-between;padding: 60rpx 40rpx 0;
}
.left-item{width: 80%;display: flex;}
.right-item{display: flex;}
.avatar-box{
    width: 72rpx;height: 72rpx;margin-right: 20rpx;
}
.m-avatar{
    display: block;width: 100%;height: 100%;border-radius: 50%;
}
.user-infos{
    padding-top: 12rpx;
    display: flex;
}
.infos-head{font-size: 36rpx;}
.r-share-btn{
    width: 44rpx;height: 44rpx;margin-top: 12rpx;
    background:url('data:image/png;base64,') no-repeat;
    background-size: 100% 100%;
}
.gender-box{
    width: 30rpx;height: 30rpx;margin: 10rpx 0 0 20rpx;
}
.gender-boy{
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% 100%;
}
.gender-girl{
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% 100%;
}
.main-container{background: #fff;padding-bottom: 40rpx;}
.ellipse-icon{
    width: 44rpx;height: 44rpx;
    /* background: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAQBAMAAADt3eJSAAAAGFBMVEUAAACFhYWIiIiHh4eHh4eHh4eIiIiIiIjVjsWQAAAACHRSTlMAGtnAnZNLR+MnSTAAAAAbSURBVAjXY6AUCBWwOLArAhnCCawBbIaUGgcAi1wB7qq3PpEAAAAASUVORK5CYII=') no-repeat;
    background-size: 100% 100%; */
    display: inline-block;
    margin: 14rpx 0 0 34rpx;
}
.main-body{
    padding: 80rpx 0 0;text-align: center;
}
.body-title{
    padding: 0 20rpx;
    font-size: 48rpx;color: #343434;font-weight: 600;margin-bottom: 96rpx;
}
.main-totals{
    display: flex;justify-content: center;align-items: center;
    width: 180rpx;border-radius: 30rpx;margin: 10rpx 0;font-size: 28rpx;background: #F4F4F4;color: #C7C7CC;
}
.hot-icon{
    width: 30rpx;height: 30rpx;margin-right: 14rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% 100%;
}
.total-container{
    padding: 0 132rpx;display: flex;
    
    justify-content: center;
}
.total-showPer{
    justify-content: space-between;
}
.percent-text{font-size: 52rpx;}
.green-font{color: #11B591;}
.dark-red-font{color: #62559D;}
.choice-box{padding: 40rpx 30rpx 0;display: flex;}
.choice-item{
    width: 336rpx;height: 320rpx;position: relative;
}
.choice-item-cont{
    width: 280rpx;height: 180rpx;
    padding-top: 70rpx;
    margin: 0 auto;
    display: flex;justify-content: center;align-items: center;
}
.left-choice-item{margin-right: 20rpx;background: #fff;box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);}
.right-choice-item{background: #fff;box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);}
.green-bg{background: #11B591;box-shadow: none;color: #fff;}
.dark-redbg{background: #62559D;box-shadow: none;color: #fff;}
.red-dot{
    display: inline-block;width: 16rpx;height: 16rpx;border-radius: 50%;
    background: #E74C49;margin-right: 12rpx;
}
.my-vote-tips{margin-top: 20rpx;font-size: 24rpx;color: #888;}
.share-mask{
    position: fixed;left: 0;top: 0;width: 100%;
    background: rgba(0, 0, 0, 0.5);z-index: 990;
}
.share-btn-container{
    position: absolute;bottom: 20rpx;width: 92%;left: 4%;
}
.wechat-container{
    border-radius: 10rpx;
    width: 100%;background: #fff;
    height: 210rpx;
    margin-bottom: 20rpx;
}
.wechat-item-box{
    background: transparent;
    margin-left: 40rpx;padding: 20rpx 0 0;display: flex;font-size: 36rpx;color: #343434;
}
.wechat-item-box::after{border: none;}
.wechat-item-box:first-child{
    border-bottom: 1px solid #f1f1f1;
}
.wechat-icon{
    width: 80rpx;height: 80rpx;margin-right: 32rpx;
}
.wechat-text{margin-top: 7rpx;}
.cancel-btn{
    width: 100%;height: 100rpx;background: #E5E7ED;color: #888;
    border-radius: 10rpx;font-size: 36rpx;display: flex;justify-content: center;align-items: center;
}
.delete-btn{
    width: 100%;height: 100rpx;background: #fff;color: #E74C49;margin-bottom: 20rpx;
    border-radius: 10rpx;font-size: 36rpx;display: flex;justify-content: center;align-items: center;
}
.imagePathBox{
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.7);
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 10;
  }
  .shengcheng{
    width: 80%;
    height: auto;padding-bottom: 24rpx;
    position: fixed;
    top: 50rpx;
    left: 50%;
    margin-left: -40%;
    z-index: 10;
  }
  .baocun{
    display: block;
  width: 48%;
  height: 92rpx;
  padding: 0;
  line-height: 92rpx;
  text-align: center;
  position: fixed;
  bottom: 110rpx;
  left: 26%;
  background: #62559D;
  color: #fff;
  font-size: 40rpx;
  border-radius: 60rpx;
  }
  button[class="baocun"]::after{
    border: 0;
  }

  .poster-view{
      background: #fff;z-index: 11;
  }
  .poster-banner{
    /* position: absolute;top: 0;left: 0; */
    width: 100%;height: 300rpx;
  }
  .poster-avatar{
      width: 120rpx;height: 120rpx;margin: 30rpx auto 0;border-radius: 50%;display: block;
  }
  .poster-text{
      width: 546rpx;height: 260rpx;margin: 0 auto;
      display: flex;justify-content: center;align-items: center;
      overflow: hidden;text-overflow: ellipsis;text-align: center;font-size: 50rpx;color: #343434;
  }
  .poster-botom-text{
      text-align: center;color: #888;font-size: 24rpx;
  }
  .poster-arrow{
      width: 20rpx;height: 12rpx;margin: 4rpx auto 20rpx;display: block;
  }
  .poster-code{
      width: 92rpx;height: 92rpx;border-radius: 50%;margin: 0 auto;
      display: block;
  }
  

.poster-close-icon{
    position: absolute;width: 60rpx;height: 60rpx;z-index: 100;
    top: 160rpx;right: 120rpx;
    display: none;
}
.vate-mask{
    position: absolute;top: 0;left: 0;width: 100%;height: 100%;
    background: rgba(0, 0, 0, 0.4);color: #fff;font-size: 80rpx;
    display: flex;justify-content: center;align-items: center;font-weight: bold;
}
.vote-selected{color: #E74C49;}