/* pages/main/main.wxss */
page{
    position: relative;overflow: hidden;
}
.main-page{
    background: #F4F4F8;
    position: relative;
}
.main-container{
    border-radius: 20rpx;
    box-shadow: 0 0 5px rgba(0, 0, 0, 0.05);padding:0 8rpx 40rpx;margin:60rpx 30rpx 0;background: #fff;
}
.main-head{
    display: flex;justify-content: space-between;padding: 60rpx 40rpx 0;
}
.left-item{width: 80%;display: flex;}
.avatar-box{
    width: 72rpx;height: 72rpx;margin-right: 20rpx;
}
.m-avatar{
    display: block;width: 100%;height: 100%;border-radius: 50%;
    background: #F4F4F8;border: 4rpx solid #efefef;
}
.user-infos{
    padding-top: 12rpx;
    display: flex;
}
.infos-head{font-size: 36rpx;}
.r-share-btn{
    width: 38rpx;height: 38rpx;margin-top: 12rpx;
    background:url('data:image/png;base64,') no-repeat;
    background-size: 100% 100%;
}
.gender-box{
    width: 30rpx;height: 30rpx;margin: 10rpx 0 0 20rpx;
}
.gender-boy{
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% 100%;
}
.gender-girl{
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% 100%;
}
.main-body{
    padding: 100rpx 0 38rpx;text-align: center;
}
.body-title{
    font-size: 48rpx;color: #343434;font-weight: 600;
    width: 80%;margin: 0 auto 120rpx;display: flex;justify-content: center;align-items: center;
}
.main-totals{
    display: flex;margin-bottom: 42rpx;justify-content: center;align-items: center;
    width: 190rpx;border-radius: 30rpx;margin: 0 auto;font-size: 28rpx;padding: 6rpx 0;background: #F4F4F4;color: #C7C7CC;
}
.hot-icon{
    width: 30rpx;height: 30rpx;margin-right: 14rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% 100%;
}
.content-box{
    display: flex;justify-content: space-between;padding: 0 20rpx;
}
.vote-box{
    width: 320rpx;height: 320rpx;position: relative;box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);font-size: 40rpx;
    color: #576B95;overflow: hidden;
}
.vote-box:first-child{margin-right: 10rpx;}
.vote-cont{
    width:260rpx;height:170rpx;margin:84rpx auto;
    text-align: center;
    display:flex;justify-content:center;align-items:center;
}
.vote-selected{color: #E74C49;}
.up-text-box{
    display: flex;justify-content: center;align-items: center;font-size: 24rpx;padding: 0 20rpx;
    padding-top: 30rpx;color: #C7C7CC;
}
.up-icon{
    width: 26rpx;height: 26rpx;margin-left: 12rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% 100%;
}
.cont-mask{
    position: absolute;left: 0;top:0;width: 100%;height: 100%;background: rgba(0,0,0, 0.4);color: #fff;
    text-align: center;font-size: 80rpx;font-weight: bold;display: flex;justify-content: center;align-items: center;
}
.shareMask{
    position: absolute;left: 0;top:0;width: 100%;height: 100%;
    background: rgba(0,0,0, 0.4);z-index: 99;
}
.share-title{text-align: center;font-size: 26rpx;}
.share-btn-box{
    display: flex;justify-content: space-between;align-items: center;
}
.share-btn{
    width: 50%;font-size: 26rpx;text-align: center;
}
.share-button{
    width: 120rpx;height: 120rpx;margin: 0 auto;border-radius: 50%;
    background: #eee;
}
.share-main{
    position: absolute;left: 3%;bottom: 0;width: 94%;padding: 30rpx 0;
}
.share-main-cont{
    background: #fff;padding: 20rpx 0;
}
.share-cancel-btn{
    font-size: 30rpx;text-align: center;border: 1px solid #ccc;
    padding: 20rpx 0;margin: 20rpx auto 0;background: #fff;
}
.imagePathBox{
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.7);
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 10;
}
.shengcheng{
    width: 80%;
    height: 80%;
    position: fixed;
    top: 50rpx;
    left: 50%;
    margin-left: -40%;
    z-index: 10;
}
.baocun{
    display: block;width: 70%;height: 72rpx;
    padding: 0;line-height: 72rpx;text-align: center;
    position: fixed;bottom: 360rpx;left: 15%;z-index: 1000;
    background: #E74C49;color: #fff;
    font-size: 28rpx;border-radius: 44rpx;
}
button[class="baocun"]::after{
    border: 0;
}
.poster-close-icon{
    position: absolute;width: 60rpx;height: 60rpx;z-index: 100;top: 140rpx;right: 100rpx;
}

.share-mask{
    position: fixed;left: 0;top: 0;width: 100%;
    background: rgba(0, 0, 0, 0.5);z-index: 99;
}
.share-btn-container{
    position: absolute;bottom: 20rpx;width: 92%;left: 4%;
}
.wechat-container{
    border-radius: 10rpx;
    width: 100%;background: #fff;height: 240rpx;margin-bottom: 20rpx;
}
.wechat-icon{
    width: 80rpx;height: 80rpx;margin-right: 32rpx;margin-top: 18rpx;
}
.moment-icon{margin-top: 4rpx;}
.wechat-text{margin-top: 18rpx;}
.cancel-btn{
    width: 100%;height: 100rpx;background: #E5E7ED;color: #888;
    border-radius: 10rpx;font-size: 36rpx;display: flex;justify-content: center;align-items: center;
}
.delete-btn{
    width: 100%;height: 100rpx;background: #fff;color: #E74C49;margin-bottom: 20rpx;
    border-radius: 10rpx;font-size: 36rpx;display: flex;justify-content: center;align-items: center;
}
button.wechat-item-box{
    margin-left: 40rpx;padding: 10rpx 0;display: flex;font-size: 36rpx;color: #343434;
    line-height: 2;background: none;
}
button::after{border: none;}
view.wechat-item-box{
    margin-left: 40rpx;padding: 12rpx 0;display: flex;font-size: 36rpx;color: #343434;
    line-height: none;background: none;
}
.wechat-item-box:first-child{
    border-bottom: 1px solid #f1f1f1;
}
.main-empty-cont{padding-top: 188rpx;}