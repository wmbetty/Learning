.thumb2{
    width: 360rpx;height: 360rpx;
    position: absolute;left: 6%;top:-2%;
    animation: thumb2 1s linear;opacity: 0;
    z-index: 15;
    animation-delay: 0.3s;
}
.vote-circle{
    width: 360rpx;height: 360rpx;
    position: absolute;left: 5%;top:-4%;
    animation: circle 1s linear;opacity: 0;

}
  @keyframes thumb1{
    50% {
      transform: scale(1);
    }
    100%{
      transform: scale(0);
      opacity: 0;
    }
  }
  @keyframes thumb2{
      15% {
          transform: scale(0.4);
          opacity: 1;
      }
      30% {
          transform: scale(0.8);
          opacity: 1;
      }
      100%{
          transform: scale(1);
          opacity: 1;
      }
  }
@keyframes circle {
    15% {
        transform: rotate(120deg);
        opacity: 1;
    }
    30% {
        transform: rotate(240deg);
        opacity: 0.5;
    }
    100%{
        transform: rotate(360deg);
        opacity: 0;
    }
}
