/* 产生动画（向外扩散变大）的圆圈 第一个圆 */
.pulse {
    position: absolute;
    width: 220rpx;
    height: 220rpx;
    left: 48px;
    top: 50px;
    border: 2px solid #E74C49;
    border-radius: 50%;
    z-index: 14;
    opacity: 0;
    animation: warn 1s linear;
    animation-delay: 0.4;
    /* box-shadow: 1px 1px 30px #E74C49;  */

}
/* 产生动画（向外扩散变大）的圆圈 第二个圆 */
.pulse1 {
    position: absolute;
    width: 300rpx;
    height: 300rpx;
    left: 30px;
    top: 32px;
    border: 2px solid #E74C49;
    border-radius: 50%;
    z-index: 11;
    opacity: 0;
    animation: warn1 0.8s linear;
    animation-delay: 1.2s;
    /* animation-iteration-count: infinite; */
}
.pulse2{
    position: absolute;
    width: 380rpx;
    height: 380rpx;
    left: 10px;
    top: 12px;
    border: 3px solid #E74C49;
    border-radius: 50%;
    z-index: 11;
    opacity: 0;
    animation: warn1 1s linear;
    animation-delay: 1.5s;
    /* animation-iteration-count: infinite; */
}

@keyframes warn {
    0% {
        transform: scale(1);
        opacity: 0.5;
    }

    100% {
        transform: scale(1.2);
        opacity: 0;
    }
}

@keyframes warn1 {
    0%{
        transform: scale(1);
        opacity: 0.5;
    }
    100%{
        transform: scale(1.2);
        opacity: 0;   /*圆形放大的同时，透明度逐渐减小为0*/
    }
}
